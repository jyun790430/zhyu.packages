{
  "name": "zhyu/packages",
  "description": "some tools for laravel",
  "keywords": [
    "laravel",
    "repository",
    "repositories",
    "eloquent",
    "database"
  ],
  "authors": [
    {
      "name": "karajanchang",
      "email": "karajanchang@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "illuminate/support": ">=5.6.0",
    "illuminate/database": ">=5.6.0",
    "illuminate/auth": ">=5.6.0"
  },
  "autoload": {
    "psr-4": {
      "Zhyu\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zhyu\\Tests\\Packages\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2.2-dev"
    },
    "laravel": {
      "providers": [
        "Zhyu\\ZhyuServiceProvider"
      ]
    }
  },
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  }
}
