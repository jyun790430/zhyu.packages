{
  "name": "zhyu/repositories",
  "description": "Laravel Repositories",
  "keywords": [
    "laravel",
    "repository",
    "repositories",
    "eloquent",
    "database"
  ],
  "authors": [
    {
      "name": "david",
      "email": "david@zhyu.com.tw"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "illuminate/support": "5.*",
    "illuminate/database": "5.*"
  },
  "autoload": {
    "psr-4": {
      "Zhyu\\Repositories\\": "src/Repositories"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zhyu\\Tests\\Repositories\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}