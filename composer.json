{
  "name": "zhyu/repositories",
  "description": "Laravel Repositories",
  "keywords": [
    "laravel",
    "repository",
    "repositories",
    "eloquent",
    "database"
  ],
  "authors": [
    {
      "name": "karajanchang",
      "email": "karajanchang@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "illuminate/support": "5.*",
    "illuminate/database": "5.*"
  },
  "autoload": {
    "psr-4": {
      "Zhyu\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zhyu\\Tests\\Repositories\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  },
  "prefer-stable": true
}